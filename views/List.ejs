<!-- views/List.ejs -->

<html>
<head>
    <title><%= pageTitle %></title>
    <% include partials/head %>
</head>
<body>
<% include partials/navigation %>
<h1>Students List</h1>
<ul>
    <% students.forEach(student => { %>
        <li>
            <p><%= student.fullName %></p>
            <p><%= student.code %></p>
            <p><%= student.fieldOfStudies %></p>
        </li>
    <% }); %>
</ul>
</body>
</html>